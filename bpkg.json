{
  "name": "echo-eval",
  "version": "0.2.0",
  "description": "Print and run BASH commands in a style that resembles someone typing at a terminal",
  "homepage": "https://github.com/kj4ezj/echo-eval",
  "author": "Zach Butler <kj4ezj+echo-eval@pm.me",
  "license": "MIT",
  "install": "make install",
  "uninstall" : "make uninstall",
  "scripts": [
    "ee.sh"
  ],
  "commands": {
    "act": "act --artifact-server-path .github/artifacts",
    "lint": "./lint.sh",
    "test": "echo '$ ./ee.test.bats' && ./ee.test.bats && echo 'Done.'"
  }
}
